{
    "name": "onofre/test_montink_25",
    "description": "Mini ERP para controle de Pedidos, Produtos, Cupons e Estoque",
    "type": "project",
    "authors": [
        {
            "name": "Onofre Souza",
            "email": "souzaonofre@gmail.com"
        }
